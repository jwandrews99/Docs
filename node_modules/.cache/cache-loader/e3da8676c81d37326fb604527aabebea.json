MC,CAAAA,OAAO,CAAGf,oBAAoB,EAApC,CACA,KAAM,CAACgB,UAAU,CAAG,EAAd,EAAoBD,OAA1B,CACA,KAAM,CAAEE,IAAF,EAAWd,SAAS,CAACe,IAAV,CAAe,2DAAf,CAA4E,CAC3FC,OAAO,CAAE,IAAM,CACbF,IAAI,GACL,CAH0F,CAA5E,CAAjB,CAKA,mBACE,oBAAC,MAAD,EACE,KAAK,CAAG,GAAED,UAAU,CAACV,KAAM,EAD7B,CAEE,WAAW,CAAC,oEAFd,eAGE,8BAAQ,SAAS,CAAET,UAAU,CAAC,oBAAD,CAAuBK,MAAM,CAACkB,UAA9B,CAA7B,eACE,2BAAK,SAAS,CAAC,WAAf,eACE,0BAAI,SAAS,CAAC,aAAd,EAA6BJ,UAAU,CAACV,KAAxC,CADF,cAEE,yBAAG,SAAS,CAAC,gBAAb,eACA,oBAAC,MAAD,mBACA,iDADA,cAEA,oBAAC,MAAD,CAAQ,SAAR,EAAkB,KAAK,CAAE,EAAzB,CAA6B,KAAK,CAAE,GAApC,EAFA,cAGA,4CAHA,cAIA,oBAAC,MAAD,CAAQ,SAAR,EAAkB,KAAK,CAAE,CAAzB,CAA4B,KAAK,CAAE,GAAnC,EAJA,cAKA,iDALA,cAMA,oBAAC,MAAD,CAAQ,SAAR,EAAkB,KAAK,CAAE,EAAzB,CAA6B,KAAK,CAAE,GAApC,EANA,cAOA,4FAPA,CADA,CAFF,cAaE,2BAAK,SAAS,CAAEJ,MAAM,CAACmB,OAAvB,eACE,oBAAC,IAAD,EACE,SAAS,CAAExB,UAAU,CACnB,qDADmB,CAEnBK,MAAM,CAACoB,UAFY,CADvB,CAKE,EAAE,CAAErB,UAAU,CAAC,OAAD,CALhB,gBADF,CAbF,CADF,CAHF,cA6BE,gCACGI,QAAQ,EAAIA,QAAQ,CAACkB,MAArB,eACC,+BAAS,SAAS,CAAErB,MAAM,CAACG,QAA3B,eACE,2BAAK,SAAS,CAAC,WAAf,eACE,2BAAK,SAAS,CAAC,KAAf,EACGA,QAAQ,CAACmB,GAAT,CAAa,CAACC,KAAD,CAAQC,GAAR,gBACZ,oBAAC,OAAD,WAAS,GAAG,CAAEA,GAAd,EAAuBD,KAAvB,EADD,CADH,CADF,CADF,CAFJ,CA7BF,CADF,CA6CD,CAED,cAAeX,CAAAA,IAAf","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport Layout from '@theme/Layout';\nimport Link from '@docusaurus/Link';\nimport useDocusaurusContext from '@docusaurus/useDocusaurusContext';\nimport useBaseUrl from '@docusaurus/useBaseUrl';\nimport styles from './styles.module.css';\nimport cogoToast from 'cogo-toast';\nimport Typist from 'react-typist';\n\n\n\nconst features = [\n  {\n    title: <>Why we chose Docusaurus</>,\n    imageUrl: 'img/undraw_docusaurus_mountain.svg',\n    description: (\n      <>\n        Docusaurus has been designed for quick and easy use, this was one of the most\n        appealing reasons why we chose this. <br></br>\n        Having the ability to easily create documents in Markdown was an amazing feature to have.\n      </>\n    ),\n  },\n  {\n    title: <>Our goal</>,\n    imageUrl: 'img/undraw_docusaurus_tree.svg',\n    description: (\n      <>\n       The goal is to create a site that is easy to maintain and update. Whilst also providing functionality to multiple devices.\n\n      </>\n    ),\n  },\n  {\n    title: <>MDX</>,\n    imageUrl: 'img/undraw_docusaurus_react.svg',\n    description: (\n      <>\n        All of our docs are created in Markdown, this makes creating or editing incredibily easy. <br></br>\n        Using MDX allows for Markdown to be seen in the browser. Making for easy reading for any user.\n      </>\n    ),\n  },\n];\n\nfunction Feature({imageUrl, title, description, button}) {\n  const imgUrl = useBaseUrl(imageUrl);\n  return (\n    <div className={classnames('col col--4', styles.feature)}>\n      {imgUrl && (\n        <div className=\"text--center\">\n          <img className={styles.featureImage} src={imgUrl} alt={title} />\n        </div>\n      )}\n      <h3>{title}</h3>\n      <p>{description}</p>\n    </div>\n  );\n}\n\nfunction Home() {\n  const context = useDocusaurusContext();\n  const {siteConfig = {}} = context;\n  const { hide } = cogoToast.info('Welcome, please bare in mind this is currently in testing', {\n    onClick: () => {\n      hide();\n    },\n  });\n  return (\n    <Layout\n      title={`${siteConfig.title}`}\n      description=\"Discite Docs, a documentation site for coders looking for snippets\">\n      <header className={classnames('hero hero--primary', styles.heroBanner)}>\n        <div className=\"container\">\n          <h1 className=\"hero__title\">{siteConfig.title}</h1>\n          <p className=\"hero__subtitle\">\n          <Typist>\n          <span>Documentation.</span>\n          <Typist.Backspace count={14} delay={400} />\n          <span>Snippets.</span>\n          <Typist.Backspace count={9} delay={400} />\n          <span>and much more!</span>\n          <Typist.Backspace count={15} delay={400} />\n          <span>Documentation, Snippets and some examples! Check us out! </span>\n          </Typist>\n          </p>\n          <div className={styles.buttons}>\n            <Link\n              className={classnames(\n                'button button--outline button--secondary button--lg',\n                styles.getStarted,\n              )}\n              to={useBaseUrl('start')}>\n              Get Started\n            </Link>\n          </div>\n        </div>\n      </header>\n      <main>\n        {features && features.length && (\n          <section className={styles.features}>\n            <div className=\"container\">\n              <div className=\"row\">\n                {features.map((props, idx) => (\n                  <Feature key={idx} {...props} />\n                ))}\n              </div>\n            </div>\n          </section>\n        )}\n      </main>\n    </Layout>\n  );\n}\n\nexport default Home;\n"]}]}